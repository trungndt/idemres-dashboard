%html
  %head
    %title Kingsmen
    %meta{charset:"utf-8"}
    %meta{content:"width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no", name:"viewport"}
    %link{href:"https://fonts.googleapis.com/css?family=Cormorant+Garamond:300,400,500,600,700|Work+Sans:300,400,500,600,700", rel:"stylesheet"}
    %link{href:"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css", rel:"stylesheet", type:"text/css"}
    %link{href:"assets/plugins/jquery-daterange/daterangepicker.min.css", rel:"stylesheet", type:"text/css"}
    %link{href:"assets/plugins/nice-select/css/nice-select.css", rel:"stylesheet", type:"text/css"}
    %link{href:"assets/css/main.css", rel:"stylesheet", type:"text/css"}

  %body.home
    %header.container-fluid
      .row.space-0
        .col-sm-2
          %a.logo{href:"/"}
            %img.img-responsive{src:"assets/images/logo-sofitel.png"}
        .col-sm-6
          %form.navbar-form
            .form-group
              %input.form-control{type:"text", placeholder:"Search..."}
        .col-sm-3.col-sm-offset-1
          %a.logo-sub{href:"/"}
            %img{src:"assets/images/sol.png"}
            %span 
              %strong Sofitel Singapore Resort
              Carrina Yang
    %section#create-block
      %h1 Create New Block
      .create-event
        .toggel-event
        .content-toggel
          %p Create Event
        %form.form-horizontal
          .form-group 
            %label.col-sm-3.control-label Account Holder
            .col-sm-5
              %input.form-control{placeholder:"Enter Name or Code"}
          .form-group 
            %label.col-sm-3.control-label Event Reference
            .col-sm-5
              %input.form-control{placeholder:"e.g Booking/Block ID, Event Name"}
          .form-group 
            %label.col-sm-3.control-label Event Name
            .col-sm-5
              %input.form-control{placeholder:"Enter Name or Code"}
          .form-group 
            %label.col-sm-3.control-label Event Type
            .col-sm-5
              %input.form-control{placeholder:"Enter Name or Code"}
          .form-group 
            %label.col-sm-3.control-label No. of Participants
            .col-sm-5
              %input.form-control{placeholder:"Enter Name or Code"}


